<script language='text/tonyu' type='text/tonyu' data-filename='Ziki.tonyu' data-lastupdate='1697009245390' >$hp=15;
while(1) {
    $zikix=x;
    $zikiy=y;
    
    zOrder=-2;
    if(getkey("W")==1){
        $houkou=0;
        new Tama{x=x, y=y};
    }
    if(getkey("S")==1){
        $houkou=1;
        new Tama{x=x, y=y};
    }
    if(getkey("A")==1){
        $houkou=2;
        new Tama{x=x, y=y};
    }
    if(getkey("D")==1){
        $houkou=3;
        new Tama{x=x, y=y};
    }
    if(crashTo(Etama)){
        crashTo(Etama).die();
        $hp=$hp-1;
    }
    /*
    if(crashTo(Teki)){
        crashTo(Teki).die();
        $hp=$hp-3;
    }*/
    if ($hp<1){
        $hp=0;
        //all(Teki).die();
        die();
    }
    sx=x;
    sy=y;
    //ここから移動
    if (getkey(40)>0 && y<60+(7*49)) y+=3;
    if (getkey(39)>0 && x<60+(7*49)) x+=3;
    if (getkey(38)>0 && y>60) y-=3; 
    if (getkey(37)>0 && x>60) x-=3; 
    if ((crashTo(Tile))){
        x=sx;
        y=sy;//移動前の値に戻す
    }
    if ((crashTo(Ntile))){
        x=sx;
        y=sy;//移動前の値に戻す
    }
update();}</script>

